<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>MainServlet2</servlet-name>
		<servlet-class>com.test.MainServlet2</servlet-class>
		<init-param>
			<param-name>shutdown-on-unload</param-name>
			<param-value>true</param-value>
		</init-param>
		<load-on-startup>0</load-on-startup>
	</servlet>
	<servlet>
		<servlet-name>MainServlet2</servlet-name>
		<display-name>MainServlet2</display-name>
		<description></description>
		<servlet-class>com.test.MainServlet2</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>mainServlet2</servlet-name>
		<display-name>mainServlet2</display-name>
		<description></description>
		<servlet-class>mainServlet2</servlet-class>
	</servlet>

	<servlet-mapping>
  <servlet-name>MainServlet2</servlet-name>
  <url-pattern>/MainServlet2/*</url-pattern>
</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MainServlet2</servlet-name>
		<url-pattern>/MainServlet2</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>mainServlet2</servlet-name>
		<url-pattern>/mainServlet2</url-pattern>
	</servlet-mapping>
</web-app>